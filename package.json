{
  "name": "angular-test-explorer",
  "displayName": "Angular Test Explorer",
  "description": "Run your Example tests in the Sidebar of Visual Studio Code",
  "icon": "img/icon.png",
  "author": "Patricio Ferraggi <pattferraggi@gmail.com>",
  "publisher": "raagh",
  "version": "0.0.1",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": ""
  },
  "bugs": {
    "url": ""
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "test",
    "testing"
  ],
  "main": "out/main.js",
  "scripts": {
    "postinstall": "node ./node_modules/vscode/bin/install",
    "clean": "rimraf out *.vsix",
    "build": "tsc",
    "watch": "tsc -w",
    "rebuild": "npm run clean && npm run build",
    "package": "vsce package",
    "publish": "vsce publish",
    "test": "jest"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "~0.13.1",
    "@angular/cli": "~7.3.1",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "request-promise": "^4.2.4",
    "tslib": "^1.9.3",
    "vscode-test-adapter-api": "^1.3.0",
    "vscode-test-adapter-util": "^0.6.3"
  },
  "devDependencies": {
    "@types/decamelize": "^1.2.0",
    "@types/express": "~4.16.1",
    "@types/jasmine": "~3.3.9",
    "@types/jasminewd2": "~2.0.6",
    "@types/jest": "^24.0.9",
    "@types/karma": "^3.0.2",
    "@types/node": "~10.12.26",
    "@types/semver": "~5.5.0",
    "jest": "^24.1.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.3",
    "vsce": "^1.57.0",
    "vscode": "^1.1.29"
  },
  "engines": {
    "vscode": "^1.23.0"
  },
  "extensionDependencies": [
    "hbenl.vscode-test-explorer"
  ],
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Example Test Explorer configuration",
      "properties": {
        "exampleExplorer.logpanel": {
          "description": "write diagnotic logs to an output panel",
          "type": "boolean",
          "scope": "resource"
        },
        "exampleExplorer.logfile": {
          "description": "write diagnostic logs to the given file",
          "type": "string",
          "scope": "resource"
        }
      }
    }
  }
}
